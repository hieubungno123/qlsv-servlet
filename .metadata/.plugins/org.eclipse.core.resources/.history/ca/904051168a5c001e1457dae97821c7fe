package test;

import user.Book;
import user.BorrowBookManagement;
import user.PersonReadBook;

public class SearchReader {
	private BorrowBookManagement[] details = new BorrowBookManagement[100];
	private PersonReadBook[] personReadBooks = new PersonReadBook[100];
	
	public void searchID(int id) {
		for(int i=0;i<personReadBooks.length;i++) {
			if(personReadBooks[i].getId()==id) {
				System.out.println(details[]);
			}
		}
	}
}
