package test4;

public class Main {
public void main(String[] args) {
	Employee emp = new Employee(50000);
	HRManager mgr = new HRManager(70000);
	
	emp.work();
	System.out.println("Employee salary: " + emp.getSalary());

	mgr.work();
	System.out.println("Manager salary: " + mgr.getSalary());
	mgr.addEmployee();
}
}
